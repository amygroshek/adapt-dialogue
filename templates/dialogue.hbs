<div class="dialogue-inner component-inner" role="region" aria-label="{{_globals._components._dialogue.ariaRegion}}">
    {{> component this}}
    <div class="dialogue-widget component-widget {{#if _hasNavigationInTextArea}}dialogue-text-controls{{/if}}">

        <div class="dialogue-content">
            <div class="dialogue-content-inner">
                {{#each _items}}
                <div class="dialogue-item">
                    <div class="dialogue-image-item" style="background-image:url('{{_graphic.src}}');" aria-label="Image of {{_graphic.caption}}">
                      <!-- <img src="{{_graphic.src}}" data-large="{{_graphic.large}}" data-small="{{_graphic.small}}"{{#if _graphic.alt}} aria-label="{{_graphic.alt}}" tabindex="0"{{else}} class="a11y-ignore" aria-hidden="true" tabindex="-1"{{/if}}/> -->
                      {{#if _graphic.caption}}
                        <div class="graphic-caption">{{{_graphic.caption}}}</div>
                      {{/if}}
                      {{#if _graphic.attribution}}
                        <div class="graphic-attribution">{{{_graphic.attribution}}}</div>
                      {{/if}}
                    </div>
                    <div class="dialogue-content-item {{#if _isVisited}}visited{{/if}}" data-index="{{@index}}">
                      <div class="dialogue-content-title">
                          <div class="dialogue-content-title-inner h5" role="heading" aria-level="5" tabindex="0">
                              {{{title}}}
                          </div>
                      </div>
                      <div class="dialogue-content-body">
                          <div class="dialogue-content-body-inner">
                              {{{a11y_text body}}}
                          </div>
                      </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>

        <div class="clearfix"></div>

    </div>
</div>
